{
  "name": "@ubi/performance",
  "version": "1.0.0",
  "private": true,
  "description": "k6 performance testing suite for UBI",
  "scripts": {
    "test:load": "k6 run dist/load-test.js",
    "test:stress": "k6 run dist/stress-test.js",
    "test:spike": "k6 run dist/spike-test.js",
    "test:soak": "k6 run dist/soak-test.js",
    "test:api": "k6 run dist/api-test.js",
    "test:all": "npm run build && k6 run dist/index.js",
    "build": "webpack --config webpack.config.js",
    "dev": "webpack --watch --config webpack.config.js"
  },
  "devDependencies": {
    "@types/k6": "^0.48.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "copy-webpack-plugin": "^11.0.0",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0"
  }
}
